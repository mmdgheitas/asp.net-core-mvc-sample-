                                        <label for="name">نام *</label>
                                        <input id="uname" name="name" type="text" data-error=".errorTxt1" value="@Model.name">
                                        <small class="errorTxt1"></small>
                                    </div>
                                    <div class="input-field col s12">
                                        <label for="pris">قیمت *</label>
                                        <input id="cemail" type="number" name="pris" data-error=".errorTxt2" value="@Model.pris">
                                        <small class="errorTxt2"></small>
                                    </div>
