                                        <label for="code_mahsoul">کد محصول *</label>
                                        <input id="code" type="text" name="code_mahsoul" data-error=".errorTxt3" value="@Model.code_mahsoul">
                                        <small class="errorTxt3"></small>
                                    </div>
                                    <div class="input-field col s12">
                                        <label for="size">سایز *</label>
                                        <input id="cpassword" type="text" name="size" data-error=".errorTxt4" value="@Model.size">
                                        <small class="errorTxt4"></small>
                                    </div>
                                    <div class="input-field col s12">
                                        <label for="color">رنگ *</label>
                                        <input id="curl" type="text" name="color" data-error=".errorTxt5" value="@Model.color">
                                        <small class="errorTxt5"></small>
                                    </div>
                                    @*<div class="col s12">
                                            <label for="crole">نقش *</label>
                                            <div class="input-field">
                                                <select class="error" id="crole" name="crole" data-error=".errorTxt6" required>
                                                    <option value="">نمایه خود را انتخاب کنید</option>
                                                    <option value="1">مدیر</option>
                                                    <option value="2">توسعه دهنده</option>
                                                    <option value="3">کسب و کار</option>
                                                </select>
                                                <small class="errorTxt6"></small>
                                            </div>
                                        </div>*@
                                    <div class="input-field col s12">
                                        <textarea id="ccomment" name="detail" class="materialize-textarea validate" data-error=".errorTxt7">@Model.detail</textarea>
                                        <label for="ccomment">نظر شما *</label>
                                        <small class="errorTxt7"></small>
                                    </div>
                                    <div class="col s12">
                                        <p>موجودی </p>
                                        @if (Model.exist == true)
                                        {
                                            <label>
                                                <input name="exist" type="radio" value="true" checked />
                                                <span>موجود</span>
                                            </label>
                                            <label>
                                                <input name="exist" type="radio" value="false" />
                                                <span>ناموجود</span>
                                            </label>
                                        }

                                        else
                                        {
                                            <label>
                                                <input name="exist" type="radio" value="true" />
                                                <span>موجود</span>
                                            </label>
                                            <label>
                                                <input name="exist" value="false" type="radio" checked />
                                                <span>نا موجود</span>
                                            </label>
                                        }

                                        <div class="input-field">
                                            <small class="errorTxt8"></small>
                                        </div>
                                    </div>
